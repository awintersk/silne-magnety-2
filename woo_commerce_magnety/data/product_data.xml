<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value
              eval="[('module', '=', 'woo_commerce_ept'),
                     '|', ('name', '=', 'product_woo_shipping_ept'),
                          ('name', '=', 'product_woo_fees_ept')]"/>
        </function>
        <value eval="{'noupdate': False}" />
    </function>

    <record id="woo_commerce_ept.product_woo_shipping_ept" model="product.product">
        <field name="name">Doprava</field>
    </record>

    <record id="woo_commerce_ept.product_woo_fees_ept" model="product.product">
        <field name="name">Dobierka</field>
    </record>

    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value
              eval="[('module', '=', 'woo_commerce_ept'),
                     '|', ('name', '=', 'product_woo_shipping_ept'),
                          ('name', '=', 'product_woo_fees_ept')]"/>
        </function>
        <value eval="{'noupdate': True}" />
    </function>

</odoo>
