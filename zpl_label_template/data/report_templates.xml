    <template id="zpl_label_pallet_view">
<t t-foreach="docs.filtered(lambda el: el.packaging_id.packing_type == 'pallet')" t-as="pallet">
<t t-set="company" t-value="env.company"/>
<t t-set="row_position" t-value="270"/>
^XA
^LL1200
^ML2400
^PW768

^A0,30,30^FO20,40^FD<t t-esc="company.name"/>^FS
^A0,30,30^FO20,80^FD<t t-esc="company.street"/>^FS
^A0,30,30^FO20,110^FD
<t t-esc="company.zip"/>
<t t-esc="company.city"/>^FS
^A0,30,30^FO20,140^FD<t t-esc="company.country_id.name"/>^FS

^FO360,40^BY2
^BCN,80,Y,N,N
^FD<t t-esc="pallet.name"/>^FS

^FO0,190^GB784,3,3^FS

^A0,30,30^FO20,220^FD#Pacs^FS
^A0,30,30^FO240,220^FDPieces^FS
^A0,30,30^FO400,220^FDGr. Weight^FS
^A0,30,30^FO640,220^FDVolume^FS

<t t-set="boxes_data" t-value="pallet.get_boxes()"/>
<t t-set="boxes" t-value="boxes_data[0]"/>
<t t-set="total_data" t-value="boxes_data[1]"/>

<t t-foreach="boxes" t-as="box_key">
^A0,30,30^FO20,<t t-esc="row_position"/>^FD<t t-esc="boxes[box_key]['name']"/>^FS
^A0,30,30^FO240,<t t-esc="row_position"/>^FD<t t-esc="boxes[box_key]['qty']"/>^FS
^A0,30,30^FO400,<t t-esc="row_position"/>^FD<t t-esc="boxes[box_key]['weight']"/>^FS
^A0,30,30^FO640,<t t-esc="row_position"/>^FD<t t-esc="boxes[box_key]['volume']"/>^FS
<t t-set="row_position" t-value="row_position+40"/>
</t>

^FO0,<t t-esc="row_position+10"/>^GB784,3,3^FS
<t t-set="row_position" t-value="row_position+40"/>

^A0,30,30^FO240,<t t-esc="row_position"/>^FD<t t-esc="total_data['qty']"/>^FS
^A0,30,30^FO400,<t t-esc="row_position"/>^FD<t t-esc="total_data['weight']"/>^FS
^A0,30,30^FO640,<t t-esc="row_position"/>^FD<t t-esc="total_data['volume']"/>^FS
^XZ
</t>
    </template>

