    <template id="zpl_label_pandora_product_view">
<t t-foreach="docs" t-as="product">
^XA
^LL200
^ML400
^PW255

^A0,12^FO100,15,0^FD<t t-esc="product.categ_id.name or product.name or ''"/>^FS
^FO0,35^GB255,1,1^FS
<t t-if="product.default_code">
^A0,14^FO70,45^FD<t t-esc="product.default_code"/>^FS
</t>
<t t-if="product.barcode">
^FO35,65^BY1
^BCN,65,Y,N,N
^FD<t t-esc="product.barcode"/>^FS
</t>
^XZ
</t>
    </template>

